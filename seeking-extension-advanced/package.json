{
  "name": "seeking-auto-detector-pro",
  "version": "2.0.0",
  "description": "Advanced Chrome extension for Seeking.com automation",
  "main": "src/background/service-worker.js",
  "type": "module",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "test": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "playwright test",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.{js,jsx,css,html}",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "package": "npm run build && zip -r dist/extension.zip dist/ -x '*.map' -x 'stats.json'",
    "clean": "rm -rf dist coverage .cache",
    "security": "npm audit && snyk test",
    "benchmark": "node tests/performance/benchmark.js",
    "docs": "jsdoc -c jsdoc.config.json"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.2.0",
    "crypto-js": "^4.1.1",
    "dexie": "^3.2.3",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.21.0",
    "@babel/preset-react": "^7.18.6",
    "@playwright/test": "^1.31.0",
    "@types/chrome": "^0.0.210",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.4.3",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sinon": "^15.0.1",
    "snyk": "^1.1100.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/seeking-extension-pro.git"
  },
  "keywords": [
    "chrome-extension",
    "automation",
    "ai",
    "machine-learning",
    "seeking"
  ],
  "author": "Ruben Hassid",
  "license": "MIT",
  "private": true
}